{
  "name": "samples",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "db:gen": "prisma generate --schema ./libs/prisma-clients/${SERVICE}/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema ./libs/prisma-clients/${SERVICE}/prisma/schema.prisma",
    "db:push": "prisma db push --schema ./libs/prisma-clients/${SERVICE}/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema ./libs/prisma-clients/${SERVICE}/prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema ./libs/prisma-clients/${SERVICE}/prisma/schema.prisma --",
    "postinstall": "SERVICE=inventory npm run db:gen && SERVICE=user npm run db:gen"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.8.1",
    "amqp-connection-manager": "^4.1.10",
    "amqplib": "^0.10.3",
    "prisma": "^4.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/devkit": "15.4.8",
    "@nrwl/eslint-plugin-nx": "15.4.8",
    "@nrwl/jest": "15.4.8",
    "@nrwl/linter": "15.4.8",
    "@nrwl/nest": "15.4.8",
    "@nrwl/node": "15.4.8",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/webpack": "15.4.8",
    "@nrwl/workspace": "15.4.8",
    "@types/faker": "^6.6.9",
    "@types/jest": "28.1.1",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "@faker-js/faker": "^7.6.0",
    "jest": "28.1.1",
