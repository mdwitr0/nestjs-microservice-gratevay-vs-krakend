generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "linux-musl"]
  previewFeatures = ["interactiveTransactions", "tracing"]
  output   = "../../../../node_modules/.prisma/inventory-client"
}

datasource db {
  provider = "postgres"
  url      = env("INVENTORY_DB_URL")
}
