generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native"]
  output   = "../../../../node_modules/.prisma/inventory-client"
}

datasource db {
  provider = "postgres"
  url      = env("INVENTORY_DB_URL")
}

model Product {
  id        String   @id @default(cuid())
  name      String
  price     Float
  creatorId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  deletedAt DateTime?
}
